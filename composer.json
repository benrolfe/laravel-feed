{
	"name": "handcoded/feed",
	"description": "HandCoded Feed package for Laravel.",
	"homepage": "https://handcodedstudio.co.uk/",
	"keywords": ["laravel", "feed", "generator", "rss", "atom"],
	"license": "MIT",
	"require": {
    "php": ">=7.3",
    "illuminate/support": "^10.0",
    "illuminate/filesystem": "^10.0"
	},
	"require-dev": {
    "laravel/framework": "^10.0",
    "phpunit/phpunit": "^9.3",
		"orchestra/testbench-core": "^6.0"
	},
	"autoload": {
		"psr-0": {
			"HandCoded\\Feed": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"HandCoded\\Feed\\FeedServiceProvider"
			],
			"aliases": {
        "Feed": "HandCoded\\Feed\\Feed"
      }
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
